<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToolBar?>


<ToolBar layoutY="7.0" prefHeight="3.0" prefWidth="600.0"
         xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="sami.talkaddict.application.AppBarController">
    <Button fx:id="loginButton" mnemonicParsing="false" onAction="#onLoginClick" prefWidth="100.0" text="Login" />
    <Separator prefWidth="30.0" />
    <Button fx:id="registerButton" mnemonicParsing="false" onAction="#onRegisterClick" prefWidth="100.0" text="Register" />
</ToolBar>
